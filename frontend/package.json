{
  "name": "commercialapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:voyagemanager": "jest apps/voyagemanager",
    "test:registers": "jest apps/registers",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 concurrently --names \"CHA,REG,HOME,ACC,VOY\" --prefix \"[{name}]\" --prefix-colors \"blue,green,yellow,magenta,cyan\" \"npm run dev:chartering\" \"npm run dev:registers\" \"npm run dev:home\" \"npm run dev:accounting\" \"npm run dev:voyagemanager\"",
    "dev:chartering": "nx run chartering:serve --port=3000",
    "dev:registers": "nx run registers:serve --port=3001",
    "dev:home": "nx run home:serve --port=3002",
    "dev:accounting": "nx run accounting:serve --port=3003",
    "dev:voyagemanager": "nx run voyagemanager:serve --port=3004",
    "dev:all": "npm run dev",
    "dev:quick": "concurrently \"npm run dev:home\" \"npm run dev:accounting\"",
    "build:all": "npm run build:chartering && npm run build:registers && npm run build:home && npm run build:accounting && npm run build:voyagemanager",
    "build:chartering": "npx nx build chartering",
    "build:registers": "npx nx build registers",
    "build:home": "npx nx build home",
    "build:accounting": "npx nx build accounting",
    "build:voyagemanager": "npx nx build voyagemanager",
    "clean": "Remove-Item -Recurse -Force node_modules, apps/*/node_modules, apps/*/.next -ErrorAction SilentlyContinue",
    "health": "curl http://localhost:3000/health && curl http://localhost:3001/health && curl http://localhost:3002/health && curl http://localhost:3003/health && curl http://localhost:3004/health"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@commercialapp/ui": "*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-table": "^8.20.5",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react-select": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.1.0",
    "next": "^15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-map-gl": "^7.1.7",
    "react-select": "^5.10.2",
    "react-select-async-paginate": "^0.7.11",
    "recharts": "^2.8.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.5",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nx/next": "^21.4.0",
